{
  "name": "wds-block-starter",
  "version": "0.0.1",
  "description": "Tooling for WDS Block Starter",
  "main": "src/index.js",
  "scripts": {
    "start": "./node_modules/.bin/wp-scripts start",
    "build": "./node_modules/.bin/wp-scripts build",
    "lint:css": "./node_modules/.bin/wp-scripts lint-style && ./node_modules/.bin/sass-lint --config ./node_modules/@webdevstudios/css-coding-standards/.sass-lint.yml '**/*.scss' --verbose",
    "lint:js": "./node_modules/.bin/wp-scripts lint-js",
    "lint:php": "./vendor/bin/phpcs --standard=./.phpcs.xml.dist --extensions=php ./ --ignore='./build/*,./node_modules/*,./vendor/*'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WebDevStudios/wds-block-starter"
  },
  "keywords": [
    "Gutenberg",
    "Exif Data"
  ],
  "author": "Mike England",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/WebDevStudios/wds-block-starter/issues"
  },
  "homepage": "https://github.com/WebDevStudios/wds-block-starter#readme",
  "devDependencies": {
    "@webdevstudios/css-coding-standards": "^1.0.0",
    "@webdevstudios/js-coding-standards": "~1.0.2",
    "@wordpress/scripts": "^7.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.4.2",
    "ignore-emit-webpack-plugin": "^2.0.2",
    "mini-css-extract-plugin": "^0.9.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "precss": "^4.0.0",
    "style-loader": "^1.1.3"
  },
  "eslintConfig": {
    "extends": [
      "plugin:@wordpress/eslint-plugin/esnext",
      "plugin:@wordpress/eslint-plugin/react",
      "@webdevstudios/js-coding-standards"
    ]
  },
  "dependencies": {},
  "sasslintConfig": "./node_modules/@webdevstudios/css-coding-standards/.sass-lint.yml",
  "stylelint": {
    "extends": "stylelint-config-wordpress",
    "ignoreFiles": [
      "**/*.scss",
      "build/**/*.css"
    ]
  }
}
